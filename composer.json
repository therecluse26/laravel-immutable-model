{
    "name": "brighten/immutable-model",
    "description": "An Eloquent-compatible, read-only model kernel for Laravel 11",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TheRecluse26",
            "email": "therecluse26@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Brighten\\ImmutableModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brighten\\ImmutableModel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
